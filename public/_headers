# Cache JavaScript for 1 year
/*.js
  Cache-Control: public, max-age=31536000, immutable

# Cache CSS for 1 year
/*.css
  Cache-Control: public, max-age=31536000, immutable

# Cache images for 1 year
/assets/*.jpg
  Cache-Control: public, max-age=31536000, immutable

# Shorter cache for HTML files (5 minutes)
/
  Cache-Control: public, max-age=300

/* 
  Cache-Control: public, max-age=300
